{
  "name": "Todo App",
  "description": "Todo App",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "todo.html"
  },
  "icons": {
    "256": "icon_256.png"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Opens todo.html"
    }
  },
  "permissions": ["storage"]
}
